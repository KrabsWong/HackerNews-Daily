{
  "name": "hacknews-daily",
  "version": "1.0.0",
  "description": "Fetch and translate HackerNews top stories to Chinese",
  "main": "dist/index.js",
  "scripts": {
    "fetch": "ts-node src/index.ts",
    "fetch:web": "ts-node src/index.ts --web",
    "build": "tsc",
    "build:web": "cd web && npm run build",
    "build:all": "npm run build:web && npm run build",
    "start": "node dist/index.js",
    "start:web": "node dist/index.js --web"
  },
  "keywords": [
    "hackernews",
    "cli",
    "translation",
    "chinese"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "jsdom": "^27.2.0",
    "open": "^11.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
